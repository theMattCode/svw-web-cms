{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Seite",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parentPage"
    },
    "parentPage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "subPages"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "mainContents": {
      "type": "dynamiczone",
      "components": [
        "block.rich-text",
        "shared.spacing",
        "block.tagged-persons",
        "block.persons",
        "block.articles"
      ]
    },
    "leftContents": {
      "type": "dynamiczone",
      "components": [
        "block.rich-text",
        "shared.spacing",
        "block.persons",
        "block.tagged-persons"
      ]
    },
    "rightContents": {
      "type": "dynamiczone",
      "components": [
        "block.rich-text",
        "shared.spacing",
        "block.persons",
        "block.tagged-persons"
      ]
    },
    "headerContents": {
      "type": "dynamiczone",
      "components": [
        "block.sub-page-navigation",
        "block.carousel"
      ]
    }
  }
}
